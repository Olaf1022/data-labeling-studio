{
  "title": "Label Studio",
  "port": 8200,
  "debug": true,

  "editor": {
    "build_path": "../build/static",
    "debug": false,
    "interfaces": [
      "basic",
      "panel",
      "submit",
      "submit:load",
      "side-column",
      "submit:skip",
      "submit:check-empty",
      "predictions:hide"
    ]
  },

  "label_config": "../examples/chatbot_analysis/config.xml",
  "input_path": "../examples/chatbot_analysis/tasks.json",
  "output_dir": "./output",

  "instruction": "Type something to label experts!",

  "logger": {
    "version": 1,
    "formatters": {
      "json": {
        "class": "utils.CustomJsonFormatter"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "DEBUG",
        "stream": "ext://sys.stdout"
      },
      "file": {
        "class": "logging.FileHandler",
        "level": "DEBUG",
        "filename": "static/logs/service.log",
        "formatter": "json"
      }
    },
    "loggers": {
      "service": {
        "level": "DEBUG",
        "handlers": [
          "console",
          "file"
        ],
        "propagate": false
      }
    },
    "root": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    }
  }
}
